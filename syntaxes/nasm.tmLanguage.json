                {
                    "name": "variable.label",
                    "match": "^\\s*[\\w\\.\\$#@~\\?]+:"
                },
                {
                    "name": "keyword.control.directive",
                    "patterns": [
                        {
                            "name": "storage.modifier.visibility",
                            "match": "\\b(global|extern|common|static|GLOBAL|EXTERN|COMMON|STATIC)\\b"
                        },
                        {
                            "name": "keyword.control.directive.bits",
                            "match": "\\b((bits|BITS)(\\s+(16|32|64))?|(use|USE)(16|32))\\b"
                        },
                        {
                            "name": "keyword.control.directive.default",
                            "match": "\\b(default|DEFAULT)(\\s+(rel|abs|nobnd|bnd|REL|ABS|NOBND|BND))?\\b"
                        },
                        {
                            "name": "keyword.control.directive.section",
                            "match": "\\b(section|segment|SECTION|SEGMENT)\\b"
                        },
                        {
                            "name": "keyword.control.directive.absolute",
                            "match": "\\b(absolute|ABSOLUTE)\\b"
                        },
                        {
                            "name": "keyword.control.directive.prefix",
                            "match": "\\b((g|l)?(pre|post)fix|(G|L)?(PRE|POST)FIX)\\b"
                        },
                        {
                            "name": "keyword.control.directive.cpu",
                            "match": "\\b(cpu|CPU)(\\s+\\w+)?\\b"
                        },
                        {
                            "name": "keyword.control.directive.float",
                            "match": "\\b(float|FLOAT)(\\s+(((no)?daz|near|down|up|zero|default)|((NO)?DAZ|NEAR|DOWN|UP|ZERO|DEFAULT)))?\\b"
                        },
                        {
                            "name": "keyword.control.directive.primitive",
                            "match": "\\b(warning|WARNING)\\b"
                        }
                    ]
                },
                {
                    "begin": "([\\w\\.\\$#@~\\?]+)\\(",
                    "beginCaptures": {
                        "1": {
                            "name": "variable.macro"
                        }
                    },
                    "end": "\\)",
                    "patterns": [
                        {
                            "include": "#expressions"
                        }
                    ]
                },
                {
                    "name": "keyword.other.preprocessor",
                    "match": "^\\s*%(?:define|macro|DEFINE|MACRO)\\s+([\\w\\.\\$#@~\\?]+)\\s*",
                    "captures": {
                        "1": {
                            "name": "variable.macro"
                        }
                    }
                },
                {
                    "name": "keyword.other.preprocessor",
                    "match": "^\\s*%\\w+\\b"
                },
                {
                    "name": "keyword.control.psuedoInstruction",
                    "match": "\\b(uppercase|import|export|(d|res)[bwdqtoyz]|times|group|near|weak|far|org|equ)|(UPPERCASE|IMPORT|EXPORT|(D|RES)[BWDQTOYZ]|TIMES|GROUP|NEAR|WEAK|FAR|ORG|EQU)\\b"
