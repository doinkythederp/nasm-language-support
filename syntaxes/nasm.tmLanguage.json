        "expressions": {
            "patterns": [
                {
                    "name": "variable.language.x86",
                    "comment": "x86_64 register name",
                    "match": "\\b((r(8|9|1[0-5])[d|w|b]?|(ah|al|bh|cl|ch|cs|dl|dh|ds|es|fs|gs|ss|bl|sil|dil|spl|bpl)|(r|e|)(ax|cx|dx|bx|si|di|sp|bp|ip|flags))|(R(8|9|1[0-5])[D|W|B]?|(AH|AL|BH|CL|CH|CS|DL|DH|DS|ES|FS|GS|SS|BL|SIL|DIL|SPL|BPL)|(R|E|)(AX|CX|DX|BX|SI|DI|SP|BP|IP|FLAGS)))\\b"
